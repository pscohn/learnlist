<li class="list-summary" id="list-<%= list.id %>">
  <h2>
    <%= link_to list.name, list %>
  </h2>
  <div class="list-meta">
    <span class="user">by <%= link_to list.user.username, list.user %></span>
    &middot;
    <%= render partial: 'lists/save', locals: {list: list, proper: false, class_name: ''} %>
  </div>
    <%= render partial: 'lists/list_progress', locals: {progress: current_user.percent_completed(list).round} %>
</li>
